<?xml version="1.0" encoding="UTF-8"?>
<ilog.rules.studio.model.brl:ActionRule xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:ilog.rules.studio.model.brl="http://ilog.rules.studio/model/brl.ecore">
  <name>ZaraRule</name>
  <uuid>5839af69-396f-4341-a860-1aa31333000e</uuid>
  <locale>en_US</locale>
  <definition><![CDATA[if
    the brand name of 'the request' is Zara 
    and the total price of 'the request' is 5000
then
    set the discount of 'the response' to 0.05 * the total price of 'the request' ;
    print "The Discount price is:" + the discount of 'the response' ;]]></definition>
</ilog.rules.studio.model.brl:ActionRule>
